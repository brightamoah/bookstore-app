{
   "Logging": {
      "LogLevel": {
         "Default": "Warning",
         "Microsoft.AspNetCore": "Warning",
         "Microsoft.EntityFrameworkCore": "Warning"
      }
   },
   "AllowedHosts": "*",
   "ConnectionStrings": {
      "Default": "${DATABASE_URL}"
   },
   "Jwt": {
      "Key": "${JWT_SECRET}",
      "Issuer": "${JWT_ISSUER}",
      "Audience": "${JWT_AUDIENCE}"
   },
   "StaticFiles": {
      "Headers": {
         "Cache-Control": "public,max-age=31536000",
         "Access-Control-Allow-Origin": "*"
      }
   }
}
